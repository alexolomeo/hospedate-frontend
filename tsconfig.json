{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*", "global.d.ts", "src/vite-env.d.ts"],
  "exclude": ["dist"],
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "types": [
      "vitest",
      "jest",
      "@testing-library/jest-dom",
      "google.maps",
      "vite-plugin-svgr/client"
    ]
  }
}
